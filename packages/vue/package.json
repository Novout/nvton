{
	"name": "vue-nvton",
	"author": {
		"email": "novout@hotmail.com",
		"name": "Giovane Cardoso",
		"url": "https://github.com/Novout"
	},
	"license": "MIT",
	"version": "0.0.0",
	"description": "A nvton plugin for Vue 3",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/Novout/nvton"
	},
	"scripts": {
		"build": "tsdown",
		"build:prod": "tsdown"
	},
	"keywords": [
		"nvton",
		"vue"
	],
	"dependencies": {
		"nvton": "workspace:*"
	},
	"devDependencies": {
		"vue": "^3.5.22"
	},
	"files": [
		"dist",
		"package.json"
	],
	"engines": {
		"node": ">= 20"
	}
}
